      <Query>
        <DataSourceName>AN4_NEWBILLINGSUMMARY</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT SP_D.PARTNERDISPLAYNAME AS DISTRIBUTOR,
      SP_C.PARTNERDISPLAYNAME AS CARRIER,
      LTT.TRANSACTIONTYPE,
      BT.TRANSACTIONID,
      BT.CONFIRMATIONNO,      
      Products.Products.cusip,      
      sob.productName,
      sob.totalPremiums AS PremiumAmount, 
      BT.BILLINGDATE as SettlementDate
      --FROM BILLINGS.BILLINGTRANSACTIONS BT WITH(NOLOCK)
      FROM Billings.BillingTransactions BT WITH(NOLOCK)
INNER JOIN TRANSACTIONS.TRANSACTIONS TT WITH(NOLOCK) ON BT.TRANSACTIONID=TT.TRANSACTIONID
JOIN Products.ProductVersions WITH(NOLOCK) ON TT.productVersionID = ProductVersions.productVersionID
JOIN Products.Products WITH(NOLOCK) ON ProductVersions.productID = products.productID
INNER JOIN SECURITY.PARTNERS SP_D WITH(NOLOCK) ON BT.DISTRIBUTORID=SP_D.PARTNERID
INNER JOIN SECURITY.PARTNERS SP_C WITH(NOLOCK) ON BT.CARRIERID=SP_C.PARTNERID
INNER JOIN Transactions.StatusOfBusiness SOB WITH(NOLOCK) ON SOB.transactionID = TT.transactionID
INNER JOIN LOOKUPS.TRANSACTIONTYPE LTT WITH(NOLOCK) ON BT.TRANSACTIONTYPEID=LTT.TRANSACTIONTYPEID
INNER JOIN LOOKUPS.TRANSACTIONSUBTYPE LTS WITH(NOLOCK) ON BT.TRANSACTIONSUBTYPEID=LTS.TRANSACTIONSUBTYPEID
WHERE 
ISNULL(TT.ISDELETED,0)=0
AND ISNULL(TT.ISTEST,0)=0
AND MONTH(BT.BILLINGDATE)=@Month
AND YEAR(BT.BILLINGDATE)=@Year
--AND BT.TRANSACTIONTYPEID IN (1) -- ALL, APP, SUB AND LEAD
--AND BT.TRANSACTIONTYPEID IN (2) -- ALL, APP, SUB AND LEAD
AND BT.TRANSACTIONTYPEID IN (1,2) -- ALL, APP, SUB AND LEAD</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
