      <Query>
        <DataSourceName>an3prodsql01</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select convert(varchar, backup_start_date, 106 /*102*/) [Date] , cast(DATEPART(HH, backup_start_date) as varchar(2))   [Hour]
, SUM(backup_size/1024/1024) [DML], database_name from msdb..backupset
where type = 'L'
and CAST(backup_start_date as DATE) between @paramstartdate  and @paramenddate 
group by  convert(varchar, backup_start_date, 106 /*102*/)  , cast(DATEPART(HH, backup_start_date) as varchar(2))    , database_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@paramstartdate">
            <Value>=Parameters!paramstartdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@paramenddate">
            <Value>=Parameters!paramenddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
