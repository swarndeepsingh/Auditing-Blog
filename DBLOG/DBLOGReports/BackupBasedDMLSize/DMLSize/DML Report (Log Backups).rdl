  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>an3prodsql01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@paramstartdate">
            <Value>=Parameters!paramstartdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@paramenddate">
            <Value>=Parameters!paramenddate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select convert(varchar, backup_start_date, 102/*102*/) [Date] , cast(DATEPART(HH, backup_start_date) as int)   [Hour]
, SUM(backup_size/1024/1024) [DML], database_name from msdb..backupset
where type = 'L'
and CAST(backup_start_date as DATE) between @paramstartdate  and @paramenddate 
group by  convert(varchar, backup_start_date, 102/*102*/)  , cast(DATEPART(HH, backup_start_date) as int)    , database_name
order by  
convert(varchar, backup_start_date, 102/*102*/) desc  , cast(DATEPART(HH, backup_start_date) as int) desc   , database_name desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Hour">
          <DataField>Hour</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DML">
          <DataField>DML</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="database_name">
          <DataField>database_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
