<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="PerformanceCollection">
      <DataSourceReference>PerformanceCollection</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b15e0c3f-9fc8-4c23-a1b4-618564d9173c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Requests">
      <Query>
        <DataSourceName>PerformanceCollection</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_time">
            <Value>=Parameters!ParamStartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_Time">
            <Value>=Parameters!ParamEndTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@serverid">
            <Value>=Parameters!ParamServerID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@wait_type">
            <Value>=Parameters!ParamWaitType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select RP.RequestRawID, RP.session_id, RP.request_id, sl.Name, wtm.waittype [WaitType], wtm1.waittype [LastWaitType], db.DatabaseName,/* sqlhm.[text], */
cmd.commandtype, stat.requeststatename,
RP.start_time, RP.cpu_time, RP.total_elapsed_time, RP.reads, rp.writes, rp.logical_reads,  RP.MemoryMB, RP.TempDBMB
, SQLHM.[text]  [Query],  
 RP.object_id, RP.ClientIP,
case 
when RP.[Statement_StartOffSet] &gt; 0 then 
	case RP.Statement_EndOffSet 
	when -1 then	
		substring(sqlhm.[Text], (RP.Statement_StartOffSet/2) + 1, 2147483647)
	else
		substring(sqlhm.[Text], (RP.Statement_StartOffSet/2) + 1, (RP.Statement_EndOffset - RP.Statement_StartOffSet)/2)
	END
else
	case RP.statement_endoffset
	when -1 then	
		rtrim(ltrim(sqlhm.[text]))
		else
			left(sqlhm.[text], (rp.statement_endoffset/2) + 1)
		END
END as [executingStatement]
 from 
 RequestsProcessed RP with (NOLOCK)
join rtm.rtm.serverslist sl
	on sl.serverid = RP.ServerID
join performancecollection.dbo.SQLHandleMaster SQLHM
	on  SQLHM.sqlhandleid = RP.sqlhandleid
	and sqlhm.databaseid = rp.databaseid
join waittypemaster wtm
	on wtm.waittypeid = rp.waittypeid
join waittypemaster wtm1
	on wtm1.waittypeid = rp.last_waittypeid
join datacollection.dbo.[databases] db
	on db.DatabaseID = rp.databaseid
	and db.serverID = rp.serverid
join performancecollection.dbo.commandtypes cmd
	on cmd.commandtypeid = rp.commandid
join performancecollection.dbo.RequestStatusTypes Stat
	on stat.requeststatusid = rp.statusid
where wtm.waittype &lt;&gt;'BROKER_RECEIVE_WAITFOR'
and start_time between @start_time and @end_Time
and sl.serverid = @serverid
and (wtm.waittype = @wait_type or wtm1.waittype = @wait_type)
order by total_elapsed_time desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
