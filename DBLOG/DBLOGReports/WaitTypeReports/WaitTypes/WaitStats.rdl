    <DataSet Name="MaxCaptureDate">
      <Query>
        <DataSourceName>PerformanceCollection</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@paramserverid">
            <Value>=Parameters!paramServerID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     MAX(CaptureDate) AS LastUpdated
FROM         WaitStatsProcessed
WHERE     (serverid = @paramserverid)</CommandText>
      </Query>
      <Fields>
        <Field Name="LastUpdated">
          <DataField>LastUpdated</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
