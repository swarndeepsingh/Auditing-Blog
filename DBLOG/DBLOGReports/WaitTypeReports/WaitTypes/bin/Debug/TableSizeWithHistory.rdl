    <DataSet Name="GetDatabases">
      <Query>
        <DataSourceName>TableSizes</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ParamServerName">
            <Value>=Parameters!paramservername.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select DatabaseName, DatabaseID from datacollection.dbo.databases db
join rtm.rtm.serverslist srv
	on srv.serverid = db.serverid
where srv.name = @ParamServerName</CommandText>
      </Query>
      <Fields>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseID">
          <DataField>DatabaseID</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetTables">
      <Query>
        <DataSourceName>TableSizes</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startdate">
            <Value>=Parameters!paramstartdate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@enddate">
            <Value>=Parameters!paramenddate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@servername">
            <Value>=Parameters!paramservername.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@databasename">
            <Value>=Parameters!paramdatabasename.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tblproc.collectdate,srv.[name], db.DatabaseName, sch.schemaname, tbl.tablename,  sch.schemaname + '.' + tbl.tablename [FullTableName], tblproc.ReservedSizeKB, tblproc.UnusedKB, tblproc.RowsCount from rtm.rtm.serverslist srv with (NOLOCK) 
join [databases] db
	on db.serverid = srv.serverid
join [schemas] sch
	on sch.databaseid = db.databaseid
	and sch.serverid = srv.serverid
join [tables] tbl
	on tbl.serverid = srv.serverid
	and tbl.databaseid = db.databaseid
	and tbl.schemaid = sch.schemaid
join tablesizeprocessed tblproc
	on tblproc.tableid = tbl.tableid
where
 cast(tblproc.collectdate as date) between @startdate and @enddate
and srv.name = @servername
and db.databasename = @databasename
order by tblproc.ReservedSizeKB desc </CommandText>
      </Query>
      <Fields>
        <Field Name="collectdate">
          <DataField>collectdate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
