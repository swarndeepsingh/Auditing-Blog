              SQLTask:SqlStatementSource="&#xA;-- PART I&#xA;if exists (select 1 from master.sys.objects where name = 'DBLOGSYSOBJ')&#xA;BEGIN&#xA; DROP TABLE master.dbo.DBLOGSYSOBJ&#xA;END&#xA;&#xA;&#xA;create table master.dbo.DBLOGSYSOBJ&#xA;(&#xA;databaseid bigint,&#xA; ObjectID INT NOT NULL,&#xA; ObjectName nvarchar(256),&#xA; DBName nvarchar(256),&#xA; &#xA; ObjectType nvarchar(256)&#xA;)&#xA;&#xA;declare @DBNAME nvarchar(256)&#xA;declare @dbid bigint&#xA;declare CUR_OBJECTCOLLECTION CURSOR FAST_FORWARD FOR&#xA;SELECT '[' + name + ']', database_id from sys.databases&#xA;where name not in ('master','msdb','tempdb', 'model') and state=0&#xA;&#xA;&#xA;OPEN CUR_OBJECTCOLLECTION&#xA;&#xA;FETCH NEXT FROM CUR_OBJECTCOLLECTION&#xA;INTO @DBNAME, @dbid&#xA;&#xA;WHILE @@FETCH_STATUS = 0&#xA;BEGIN&#xA; INSERT INTO master.dbo.DBLOGSYSOBJ&#xA; exec ('SELECT ' + @dbid + ', obj.object_id, sch.name + ''.'' + obj.[name], ''' + @DBNAME + ''' [DBNAME], obj.type_desc [ObjectTYpe]  from ' + @DBNAME + '.SYS.objects obj join  ' + @DBNAME + '.SYS.schemas sch on sch.schema_id = obj.schema_id where type_Desc  in (''SQL_STORED_PROCEDURE'',''SQL_TRIGGER'',''VIEW'', ''SQL_SCALAR_FUNCTION'')')&#xA;&#xA;&#xA; /*select ('SELECT object_id, [name], ' + @DBNAME + ' [DBNAME], type_desc [ObjectTYpe] from ' + @DBNAME + '.SYS.objects')*/&#xA;FETCH NEXT FROM CUR_OBJECTCOLLECTION&#xA;INTO @DBNAME, @dbid&#xA;END&#xA;CLOSE CUR_OBJECTCOLLECTION&#xA;DEALLOCATE CUR_OBJECTCOLLECTION&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
