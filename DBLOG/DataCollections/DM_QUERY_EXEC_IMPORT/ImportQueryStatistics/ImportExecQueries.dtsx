                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- PART II
declare @collectiontime datetime
select @collectiontime = getdate()

select [sql].text [SQL], [sql].objectid [ObjectID], coll.ObjectName [ObjectName], coll.DBName [DBNAME], coll.ObjectType [OBJECTTYPE], [plan].query_plan [QueryPlan], er.session_id [SPID]
, er.start_time, er.status
, er.command, er.database_id, er.wait_type, er.wait_resource ,
er.wait_time, er.last_wait_type, er.cpu_time, er.total_elapsed_time, er.reads, er.writes, er.logical_reads, @collectiontime [CollectionTime], @@SERVERNAME [servername] 
,er.blocking_session_id, des.host_name, des.program_name, des.client_interface_name, des.login_name, des.memory_usage, des.row_count
from sys.dm_exec_requests er
outer apply sys.dm_exec_sql_text (er.sql_handle) [sql] 
outer apply sys.dm_exec_query_plan (er.plan_handle) [plan]
left outer join master.dbo.DBLOGSYSOBJ coll
	on coll.ObjectID = sql.objectid
	and coll.databaseid = sql.dbid
join sys.dm_exec_sessions des
	on des.session_id = er.session_id
where er.total_elapsed_time &gt; 10000 and des.is_user_process = 1</property>
